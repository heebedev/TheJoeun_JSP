package com.blgy.dto;

public class CmtDto {
	
	String nickname;
	String comment;
	String insertdate;
	String nickcheck;
	
	public CmtDto() {
		// TODO Auto-generated constructor stub
	}


	
	public CmtDto(String nickname, String comment, String insertdate, String nickcheck) {
		super();
		this.nickname = nickname;
		this.comment = comment;
		this.insertdate = insertdate;
		this.nickcheck = nickcheck;
	}


	public String getNickname() {
		return nickname;
	}

	public void setNickname(String nickname) {
		this.nickname = nickname;
	}

	public String getComment() {
		return comment;
	}

	public void setComment(String comment) {
		this.comment = comment;
	}

	public String getInsertdate() {
		return insertdate;
	}

	public void setInsertdate(String insertdate) {
		this.insertdate = insertdate;
	}



	public String getNickcheck() {
		return nickcheck;
	}



	public void setNickcheck(String nickcheck) {
		this.nickcheck = nickcheck;
	}
	
	
	
	

}
