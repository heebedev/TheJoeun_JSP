package com.blgy.dto;

public class CmtDto {
	
	int cmtseq;
	String nickname;
	String comment;
	String insertdate;
	String cmtRev;
	
	public CmtDto() {
		// TODO Auto-generated constructor stub
	}


	
	public CmtDto(int cmtseq,  String nickname, String comment, String insertdate, String cmtRev) {
		super();
		this.cmtseq = cmtseq;
		this.nickname = nickname;
		this.comment = comment;
		this.insertdate = insertdate;
		this.cmtRev = cmtRev;
	}
	
	public CmtDto(int cmtseq,  String nickname, String comment, String insertdate) {
		super();
		this.cmtseq = cmtseq;
		this.nickname = nickname;
		this.comment = comment;
		this.insertdate = insertdate;
	}
	
	
	


	public String getNickname() {
		return nickname;
	}

	public void setNickname(String nickname) {
		this.nickname = nickname;
	}

	public String getComment() {
		return comment;
	}

	public void setComment(String comment) {
		this.comment = comment;
	}

	public String getInsertdate() {
		return insertdate;
	}

	public void setInsertdate(String insertdate) {
		this.insertdate = insertdate;
	}



	public String getCmtRev() {
		return cmtRev;
	}

	public void setCmtRev(String cmtRev) {
		this.cmtRev = cmtRev;
	}

	public int getCmtseq() {
		return cmtseq;
	}



	public void setCmtseq(int cmtseq) {
		this.cmtseq = cmtseq;
	}
	
	
	
	

}
